{
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "h",
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "declaration": false,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es6"
    ],
    "module": "commonjs",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "pretty": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "target": "es2017"
  }
}